{
  "name": "FullSack-Blog",
  "version": "1.0.0",
  "description": "Buliding FullSack Blog Application Using Node.js api with mongodb",
  "main": "./backend/app.js",
  "type": "module",
  "scripts": {
    "start": "node ./backend/app.js",
    "client": "npm run dev --prefix frontend",
    "server": "nodemon ./backend/app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "Abdullahi Khalif",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.0.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "react-icons": "^4.11.0"
  }
}
